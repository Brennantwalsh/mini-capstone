<h1>Update an Image</h1>

<%= form_tag '/images', method: :post do %>
  <div class = "form-group">
    <%= label_tag :url, 'Enter url'%>
    <%= text_field_tag :url, @image.url%>
  </div>
  <div class = "form-group">
    <%= label_tag :product_id, 'Enter product_id'%>
    <%= text_field_tag :product_id, @image.product_id%>
  </div>

<% end %>